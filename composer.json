{
    "name": "tejasvaidya01/iitbtestproject",
    "description": "Project template for Drupal 8 sites.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "IITBombayWeb/acadmixdistprofile": "dev-dev-test",
        "tejasvaidya01/acadmix_event": "dev-master",
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "dev": {
            "type": "github",
            "url": "https://github.com/IITBombayWeb/acadmixdistprofile",
            "no-api": true
        },
        "dev1": {
            "type": "github",
            "url": "https://github.com/tejasvaidya01/acadmix_event",
            "no-api": true
        }
    },
    "config": {
	"process-timeout": 0
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"                
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "enable-patching": true
    }
}
