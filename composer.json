{
    "name": "tejasvaidya01/iitbtestproject",
    "description": "Project template for Drupal 8 sites.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drupal/core": "8.6.6",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "wikimedia/composer-merge-plugin": "^1.4",
        "drupal/console": "~1.0",
        "drush/drush": "^8.0",
        "IITBombayWeb/acadmixdistprofile": "~0.1.0"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "acadmixdist": {
            "type": "github",
            "url": "https://github.com/IITBombayWeb/acadmixdistprofile",
            "no-api": true
        }
    },
    "config": {
        "process-timeout": 0
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "docroot/profiles/contrib/acadmixdistprofile/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"                
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "enable-patching": true
    }
}
