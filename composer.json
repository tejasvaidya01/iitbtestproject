{
    "name": "tejasvaidya01/iitbtestproject",
    "description": "Project template for Drupal 8 sites.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "IITBombayWeb/acadmixdistprofile": "0.1.0-alpha",
        "tejasvaidya01/acadmix_event": "~0.1.0",
        "tejasvaidya01/acadmix_news": "~0.1.0",
        "tejasvaidya01/acadmix_faculty": "~0.1.0",
        "slick": "1.9.0",
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "acadmixdist": {
            "type": "github",
            "url": "https://github.com/IITBombayWeb/acadmixdistprofile",
            "no-api": true
        },
        "event": {
            "type": "github",
            "url": "https://github.com/tejasvaidya01/acadmix_event",
            "no-api": true
        },
        "news": {
            "type": "github",
            "url": "https://github.com/tejasvaidya01/acadmix_news",
            "no-api": true
        },
        "faculty": {
            "type": "github",
            "url": "https://github.com/tejasvaidya01/acadmix_faculty",
            "no-api": true
        },
        "slick": {
            "type": "package",
            "package": {
                "name": "slick",
                "version": "v1.9.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    },
    "config": {
	"process-timeout": 0
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"                
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "enable-patching": true
    }
}
